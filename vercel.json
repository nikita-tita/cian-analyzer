{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/src/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "functions": {
    "index.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
