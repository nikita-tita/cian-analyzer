{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/src/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "functions": {
    "index.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "FLASK_ENV": "production",
    "LOG_LEVEL": "INFO",
    "CACHE_ENABLED": "true"
  },
  "regions": ["iad1"]
}
